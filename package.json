{
  "name": "pca",
  "version": "5.8.0",
  "description": "Papyrus compiler app (PCA) by WushuLate",
  "private": true,
  "scripts": {
    "dev": "electron-esbuild dev",
    "build": "electron-esbuild build",
    "package": "electron-builder",
    "lint": "eslint src --ext .ts,.tsx",
    "prepare": "husky install"
  },
  "engines": {
    "node": "~16.13.1",
    "pnpm": "^6.32.4"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:Kiyozz/papyrus-compiler-app.git"
  },
  "author": "WK <kevinturmel@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@emotion/react": "^11.9.0",
    "@emotion/styled": "^11.8.1",
    "@fontsource/ibm-plex-mono": "^4.5.7",
    "@fontsource/roboto": "^4.5.5",
    "@mui/icons-material": "^5.6.2",
    "@mui/material": "^5.6.2",
    "@pca/electron-ipc": "workspace:^1.0.0",
    "@sindresorhus/is": "~4.6.0",
    "@types/lodash-es": "~4.17.6",
    "@types/minimist": "^1.2.2",
    "@types/react": "~17.0.44",
    "@types/react-dom": "~17.0.15",
    "@types/react-router": "^5.1.18",
    "@types/react-router-dom": "^5.3.3",
    "@types/react-transition-group": "^4.4.4",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "~5.20.0",
    "@typescript-eslint/parser": "~5.20.0",
    "@vercel/style-guide": "^3.0.0",
    "@vitejs/plugin-react": "^1.3.1",
    "autoprefixer": "~10.4.4",
    "classnames": "~2.3.1",
    "compare-versions": "~4.1.3",
    "debounce-fn": "~5.1.0",
    "deepmerge": "~4.2.2",
    "electron": "^18.1.0",
    "electron-builder": "~23.0.3",
    "electron-context-menu": "^3.1.2",
    "electron-default-menu": "~1.0.2",
    "electron-esbuild": "^5.0.2",
    "electron-fetch": "^1.7.4",
    "electron-log": "~4.4.6",
    "electron-store": "~8.0.1",
    "electron-util": "~0.17.2",
    "esbuild": "^0.14.37",
    "eslint": "~8.13.0",
    "eslint-plugin-prettier": "^4.0.0",
    "fast-glob": "~3.2.11",
    "history": "^5.3.0",
    "husky": "~7.0.4",
    "i18next": "~21.6.16",
    "i18next-browser-languagedetector": "~6.1.4",
    "lint-staged": "~12.4.0",
    "minimist": "^1.2.6",
    "move-file": "~3.0.0",
    "os-locale": "^6.0.2",
    "path-shorten": "^0.1.2",
    "postcss": "~8.4.12",
    "prettier": "~2.6.2",
    "prettier-plugin-tailwindcss": "^0.1.10",
    "queue": "^6.0.2",
    "react": "~17.0.2",
    "react-dom": "~17.0.2",
    "react-dropzone": "~12.0.5",
    "react-i18next": "~11.16.7",
    "react-markdown": "~8.0.3",
    "react-router-dom": "^6.3.0",
    "react-transition-group": "^4.4.2",
    "react-use-localstorage": "^3.5.3",
    "rimraf": "~3.0.2",
    "rooks": "^5.11.0",
    "rxjs": "~7.5.5",
    "tailwindcss": "~3.0.24",
    "ts-pattern": "^4.0.2",
    "type-fest": "^2.12.2",
    "typescript": "~4.6.3",
    "uuid": "^8.3.2",
    "vite": "^2.9.5"
  },
  "build": {
    "appId": "io.kiyozz.pca",
    "productName": "PCA",
    "copyright": "Copyright Â© WushuLate",
    "afterPack": "./build/after-pack.js",
    "artifactName": "${productName}.${ext}",
    "extraMetadata": {
      "name": "PCA",
      "main": "main.js"
    },
    "files": [
      {
        "from": ".",
        "filter": [
          "package.json"
        ]
      },
      {
        "from": "dist/main"
      },
      {
        "from": "dist/renderer"
      }
    ],
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true,
      "artifactName": "${productName}.Setup.${ext}"
    },
    "win": {
      "target": [
        "7z"
      ]
    },
    "mac": {
      "target": [
        "dir"
      ]
    },
    "linux": {
      "target": [
        "dir"
      ]
    },
    "directories": {
      "buildResources": "resources"
    },
    "publish": null
  },
  "lint-staged": {
    "*.{md,json,css,yaml,yml}": [
      "prettier --write"
    ],
    "*.{ts,tsx,js,mjs,mts}": [
      "eslint --fix --max-warnings=0"
    ]
  }
}
