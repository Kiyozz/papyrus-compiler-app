{
  "name": "pca",
  "version": "5.4.0",
  "description": "Papyrus compiler app (PCA) by WushuLate",
  "private": true,
  "scripts": {
    "dev": "electron-esbuild dev",
    "build": "electron-esbuild build",
    "package": "electron-builder",
    "lint": "eslint src --ext .ts,.tsx"
  },
  "engines": {
    "node": "^15.11.0",
    "pnpm": "~5.18.7"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:Kiyozz/papyrus-compiler-app.git"
  },
  "keywords": [
    "electron",
    "typescript",
    "react",
    "papyrus",
    "skyrim",
    "creation kit"
  ],
  "author": "WK <kevinturmel@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "~7.13.10",
    "@babel/plugin-proposal-class-properties": "~7.13.0",
    "@babel/plugin-proposal-nullish-coalescing-operator": "~7.13.8",
    "@babel/plugin-proposal-optional-chaining": "~7.13.8",
    "@babel/preset-env": "~7.13.10",
    "@babel/preset-react": "~7.12.13",
    "@babel/preset-typescript": "~7.13.0",
    "@fontsource/roboto": "^4.2.2",
    "@fontsource/roboto-mono": "^4.2.2",
    "@material-ui/core": "~4.11.3",
    "@material-ui/icons": "~4.11.2",
    "@material-ui/lab": "~4.0.0-alpha.57",
    "@pmmmwh/react-refresh-webpack-plugin": "~0.4.3",
    "@reach/router": "~1.3.4",
    "@sindresorhus/is": "~4.0.0",
    "@types/classnames": "~2.2.11",
    "@types/electron-devtools-installer": "~2.2.0",
    "@types/lodash-es": "~4.17.4",
    "@types/reach__router": "~1.3.7",
    "@types/react": "~16.9.56",
    "@types/react-dom": "~16.9.11",
    "@types/react-redux": "~7.1.16",
    "@types/redux-actions": "~2.6.1",
    "@types/redux-logger": "~3.0.8",
    "@types/webpack-dev-server": "~3.11.2",
    "@typescript-eslint/eslint-plugin": "~4.18.0",
    "@typescript-eslint/parser": "~4.18.0",
    "autoprefixer": "~10.2.5",
    "babel-loader": "~8.2.2",
    "chokidar": "~3.5.1",
    "classnames": "~2.2.6",
    "compare-versions": "~3.6.0",
    "css-hot-loader": "~1.4.4",
    "css-loader": "~5.1.3",
    "css-minimizer-webpack-plugin": "~1.3.0",
    "custom-electron-titlebar": "~3.2.6",
    "debounce-fn": "~4.0.0",
    "deepmerge": "~4.2.2",
    "dependency-tree": "~8.0.0",
    "electron": "~12.0.1",
    "electron-builder": "~22.10.5",
    "electron-default-menu": "~1.0.2",
    "electron-devtools-installer": "~3.1.1",
    "electron-esbuild": "~1.2.3",
    "electron-log": "~4.3.2",
    "electron-store": "~7.0.2",
    "electron-util": "~0.14.2",
    "esbuild": "0.9.3",
    "eslint": "~7.22.0",
    "eslint-config-prettier": "~8.1.0",
    "eslint-plugin-import": "~2.22.1",
    "eslint-plugin-prettier": "~3.3.1",
    "eslint-plugin-react": "~7.22.0",
    "eslint-plugin-react-hooks": "~4.2.0",
    "fast-glob": "~3.2.5",
    "file-loader": "~6.2.0",
    "find-up": "~5.0.0",
    "fork-ts-checker-webpack-plugin": "~4.1.6",
    "html-webpack-plugin": "~4.5.2",
    "husky": "~4.3.8",
    "i18next": "~19.7.0",
    "i18next-browser-languagedetector": "~6.0.1",
    "lint-staged": "~10.5.4",
    "lodash-es": "~4.17.21",
    "mini-css-extract-plugin": "~1.3.9",
    "move-file": "~2.0.0",
    "postcss": "~8.2.8",
    "postcss-loader": "~4.1.0",
    "prettier": "~2.0.5",
    "react": "~16.14.0",
    "react-dom": "~16.14.0",
    "react-dropzone": "~11.0.3",
    "react-i18next": "~11.7.4",
    "react-markdown": "~4.3.1",
    "react-refresh": "~0.9.0",
    "react-use-localstorage": "^3.5.3",
    "rimraf": "~3.0.2",
    "rxjs": "~6.6.6",
    "speed-measure-webpack-plugin": "~1.3.3",
    "tailwindcss": "~2.0.3",
    "terser-webpack-plugin": "~4.2.3",
    "type-fest": "~0.13.1",
    "typescript": "~4.2.3",
    "url-loader": "~4.1.1",
    "webpack": "~4.46.0",
    "webpack-dev-server": "~3.11.2"
  },
  "build": {
    "appId": "io.kiyozz.pca",
    "productName": "PCA",
    "copyright": "Copyright Â© WushuLate",
    "afterPack": "./build/after-pack.js",
    "artifactName": "${productName}.${ext}",
    "extraMetadata": {
      "name": "PCA",
      "main": "main.js"
    },
    "files": [
      {
        "from": ".",
        "filter": [
          "package.json"
        ]
      },
      {
        "from": "dist/main"
      },
      {
        "from": "dist/renderer"
      }
    ],
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true,
      "artifactName": "${productName}.Setup.${ext}"
    },
    "win": {
      "target": [
        "nsis",
        "7z"
      ]
    },
    "mac": {
      "target": [
        "zip"
      ]
    },
    "linux": {
      "target": [
        "zip"
      ]
    },
    "directories": {
      "buildResources": "resources"
    },
    "publish": null
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{md,json,css,yaml,yml}": [
      "prettier --write"
    ],
    "*.{ts,tsx,js,mjs}": [
      "eslint --fix"
    ]
  },
  "dependencies": {
    "electron-fetch": "^1.7.3"
  }
}
