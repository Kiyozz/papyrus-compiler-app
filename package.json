{
  "name": "pca",
  "version": "5.8.0",
  "description": "Papyrus compiler app (PCA) by WushuLate",
  "private": true,
  "scripts": {
    "dev": "electron-esbuild dev",
    "build": "electron-esbuild build",
    "package": "electron-builder",
    "lint": "eslint src --ext .ts,.tsx",
    "prepare": "husky install"
  },
  "engines": {
    "node": "16",
    "pnpm": "7"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:Kiyozz/papyrus-compiler-app.git"
  },
  "author": "WK <kevinturmel@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@emotion/react": "^11.10.0",
    "@emotion/styled": "^11.10.0",
    "@fontsource/ibm-plex-mono": "^4.5.10",
    "@fontsource/roboto": "^4.5.7",
    "@mui/icons-material": "^5.8.4",
    "@mui/material": "^5.9.3",
    "@pca/electron-ipc": "workspace:^1.0.0",
    "@sindresorhus/is": "~5.3.0",
    "@types/lodash-es": "~4.17.6",
    "@types/minimist": "^1.2.2",
    "@types/react": "~18.0.15",
    "@types/react-dom": "~18.0.6",
    "@types/react-router": "^5.1.18",
    "@types/react-router-dom": "^5.3.3",
    "@types/react-transition-group": "^4.4.5",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "~5.32.0",
    "@typescript-eslint/parser": "~5.32.0",
    "@vercel/style-guide": "^3.0.0",
    "@vitejs/plugin-react": "^2.0.0",
    "autoprefixer": "~10.4.8",
    "classnames": "~2.3.1",
    "compare-versions": "~4.1.3",
    "debounce-fn": "~5.1.2",
    "deepmerge": "~4.2.2",
    "electron": "^20.0.1",
    "electron-builder": "~23.3.3",
    "electron-context-menu": "^3.3.0",
    "electron-default-menu": "~1.0.2",
    "electron-esbuild": "^6.0.0",
    "electron-fetch": "^1.7.4",
    "electron-log": "~4.4.8",
    "electron-store": "~8.1.0",
    "electron-util": "~0.17.2",
    "esbuild": "^0.14.53",
    "eslint": "~8.21.0",
    "eslint-plugin-prettier": "^4.2.1",
    "fast-glob": "~3.2.11",
    "history": "^5.3.0",
    "husky": "~8.0.1",
    "i18next": "~21.8.16",
    "i18next-browser-languagedetector": "~6.1.4",
    "lint-staged": "~13.0.3",
    "minimist": "^1.2.6",
    "move-file": "~3.0.0",
    "os-locale": "^6.0.2",
    "path-shorten": "^0.1.2",
    "postcss": "~8.4.14",
    "prettier": "~2.7.1",
    "prettier-plugin-tailwindcss": "^0.1.13",
    "queue": "^6.0.2",
    "react": "~18.2.0",
    "react-dom": "~18.2.0",
    "react-dropzone": "^14.2.2",
    "react-i18next": "~11.18.3",
    "react-markdown": "~8.0.3",
    "react-router-dom": "^6.3.0",
    "react-transition-group": "^4.4.5",
    "react-use-localstorage": "^3.5.3",
    "rimraf": "~3.0.2",
    "rooks": "^6.2.0",
    "rxjs": "~7.5.6",
    "tailwindcss": "~3.1.7",
    "ts-pattern": "^4.0.5",
    "type-fest": "^2.18.0",
    "typescript": "~4.7.4",
    "uuid": "^8.3.2",
    "vite": "^3.0.4"
  },
  "build": {
    "appId": "io.kiyozz.pca",
    "productName": "PCA",
    "copyright": "Copyright Â© WushuLate",
    "afterPack": "./build/after-pack.js",
    "artifactName": "${productName}.${ext}",
    "extraMetadata": {
      "name": "PCA",
      "main": "main.js"
    },
    "files": [
      {
        "from": ".",
        "filter": [
          "package.json"
        ]
      },
      {
        "from": "dist/main"
      },
      {
        "from": "dist/renderer"
      }
    ],
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true,
      "artifactName": "${productName}.Setup.${ext}"
    },
    "win": {
      "target": [
        "7z"
      ]
    },
    "mac": {
      "target": [
        "dir"
      ]
    },
    "linux": {
      "target": [
        "dir"
      ]
    },
    "directories": {
      "buildResources": "resources"
    },
    "publish": null
  },
  "lint-staged": {
    "*.{md,json,css,yaml,yml}": [
      "prettier --write"
    ],
    "*.{ts,tsx,js,mjs,mts}": [
      "eslint --fix --max-warnings=0"
    ]
  }
}
