    "typeface-roboto-mono": "^0.0.75",
    "typescript": "^3.9.7",
    "webpack": "^4.44.1"
  },
  "electronWebpack": {
    "renderer": {
      "webpackConfig": "webpack.renderer.additions.js"
    },
    "main": {
      "webpackConfig": "webpack.main.additions.js"
    }
  },
  "build": {
    "appId": "io.kiyozz.pca",
    "productName": "PCA",
    "copyright": "Copyright Â© WushuLate",
    "extraMetadata": {
      "name": "PCA"
    }
  },
  "husky": {
