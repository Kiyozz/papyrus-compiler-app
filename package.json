{
  "name": "pca",
  "version": "5.8.0",
  "description": "Papyrus compiler app (PCA) by WushuLate",
  "private": true,
  "scripts": {
    "dev": "electron-esbuild dev",
    "build": "electron-esbuild build",
    "package": "electron-builder",
    "lint": "eslint src --ext .ts,.tsx",
    "prepare": "husky install"
  },
  "engines": {
    "node": "16.9.1",
    "pnpm": "^6.26.1"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:Kiyozz/papyrus-compiler-app.git"
  },
  "keywords": [
    "electron",
    "typescript",
    "react",
    "papyrus",
    "skyrim",
    "creation kit"
  ],
  "author": "WK <kevinturmel@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@fontsource/roboto": "^4.5.1",
    "@fontsource/roboto-mono": "^4.5.0",
    "@material-ui/core": "~4.12.3",
    "@material-ui/icons": "~4.11.2",
    "@sindresorhus/is": "~4.3.0",
    "@types/lodash-es": "~4.17.5",
    "@types/react": "~17.0.38",
    "@types/react-dom": "~17.0.11",
    "@types/react-router": "^5.1.17",
    "@types/react-router-dom": "^5.3.2",
    "@types/react-transition-group": "^4.4.4",
    "@typescript-eslint/eslint-plugin": "~5.10.0",
    "@typescript-eslint/parser": "~5.10.0",
    "@vitejs/plugin-react-refresh": "^1.3.6",
    "autoprefixer": "~10.4.2",
    "classnames": "~2.3.1",
    "compare-versions": "~4.1.3",
    "debounce-fn": "~5.1.0",
    "deepmerge": "~4.2.2",
    "electron": "~16.0.7",
    "electron-builder": "~22.14.5",
    "electron-context-menu": "^3.1.1",
    "electron-default-menu": "~1.0.2",
    "electron-esbuild": "^4.0.1",
    "electron-fetch": "^1.7.4",
    "electron-log": "~4.4.4",
    "electron-store": "~8.0.1",
    "electron-util": "~0.17.2",
    "esbuild": "^0.14.11",
    "eslint": "~8.7.0",
    "eslint-config-prettier": "~8.3.0",
    "eslint-plugin-import": "~2.25.4",
    "eslint-plugin-prettier": "~4.0.0",
    "eslint-plugin-react": "~7.28.0",
    "eslint-plugin-react-hooks": "~4.3.0",
    "fast-glob": "~3.2.11",
    "history": "^5.2.0",
    "husky": "~7.0.4",
    "i18next": "~21.6.6",
    "i18next-browser-languagedetector": "~6.1.2",
    "lint-staged": "~12.2.0",
    "move-file": "~3.0.0",
    "os-locale": "^6.0.2",
    "path-shorten": "^0.1.2",
    "postcss": "~8.4.5",
    "prettier": "~2.5.1",
    "queue": "^6.0.2",
    "react": "~17.0.2",
    "react-dom": "~17.0.2",
    "react-dropzone": "~11.5.1",
    "react-i18next": "~11.15.3",
    "react-markdown": "~7.1.2",
    "react-router-dom": "^6.2.1",
    "react-transition-group": "^4.4.2",
    "react-use-localstorage": "^3.5.3",
    "rimraf": "~3.0.2",
    "rooks": "^5.8.0",
    "rxjs": "~7.5.2",
    "tailwindcss": "~3.0.15",
    "ts-pattern": "^3.3.4",
    "type-fest": "^2.9.0",
    "typescript": "~4.5.4",
    "vite": "^2.7.12"
  },
  "build": {
    "appId": "io.kiyozz.pca",
    "productName": "PCA",
    "copyright": "Copyright Â© WushuLate",
    "afterPack": "./build/after-pack.js",
    "artifactName": "${productName}.${ext}",
    "extraMetadata": {
      "name": "PCA",
      "main": "main.js"
    },
    "files": [
      {
        "from": ".",
        "filter": [
          "package.json"
        ]
      },
      {
        "from": "dist/main"
      },
      {
        "from": "dist/renderer"
      }
    ],
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true,
      "artifactName": "${productName}.Setup.${ext}"
    },
    "win": {
      "target": [
        "7z"
      ]
    },
    "mac": {
      "target": [
        "dir"
      ]
    },
    "linux": {
      "target": [
        "dir"
      ]
    },
    "directories": {
      "buildResources": "resources"
    },
    "publish": null
  },
  "lint-staged": {
    "*.{md,json,css,yaml,yml}": [
      "prettier --write"
    ],
    "*.{ts,tsx,js,mjs}": [
      "eslint --fix --max-warnings=0"
    ]
  }
}
