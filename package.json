{
  "name": "pca",
  "version": "4.5.0",
  "description": "Papyrus compiler app (PCA) by WushuLate",
  "private": true,
  "scripts": {
    "start": "node scripts/dev.mjs",
    "build": "node scripts/build.mjs",
    "package": "electron-builder -p=never",
    "lint": "eslint src --ext .ts,.tsx",
    "release:dry": "semantic-release --dry-run --no-ci"
  },
  "engines": {
    "node": "~14.15.3"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:Kiyozz/papyrus-compiler-app.git"
  },
  "keywords": [
    "electron",
    "typescript",
    "react",
    "papyrus",
    "skyrim",
    "creation kit"
  ],
  "author": "WK <kevinturmel@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "@sindresorhus/is": "~3.1.2",
    "deepmerge": "~4.2.2",
