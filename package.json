{
  "name": "pca",
  "version": "4.1.0",
  "description": "Papyrus compiler app by WushuLate",
  "private": true,
  "scripts": {
    "start": "electron-webpack dev",
    "build": "pnpm build:le",
    "prebuild:le": "rimraf dist",
    "prebuild:se": "rimraf dist",
    "build:le": "cross-env APP_VERSION=LE electron-webpack",
    "build:se": "cross-env APP_VERSION=SE electron-webpack",
    "prepackage:le": "pnpm build:le",
    "package:le": "electron-builder -p=never --config.productName='PCA LE'",
    "prepackage:se": "pnpm build:se",
    "package:se": "electron-builder -p=never --config.productName='PCA SE'",
    "lint": "eslint src/* --ext .ts,.tsx",
    "release": "semantic-release"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Kiyozz/papyrus-compiler-app.git"
  },
  "keywords": [
    "electron",
    "typescript",
    "react",
    "papyrus",
    "skyrim",
    "creation kit"
  ],
  "author": "WK <kevinturmel@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "@sindresorhus/is": "^3.1.2",
    "deepmerge": "^4.2.2",
    "electron-better-ipc": "^1.1.1",
    "electron-default-menu": "^1.0.2",
    "electron-log": "^4.2.4",
    "electron-store": "^6.0.0",
    "electron-util": "^0.14.2",
    "fast-glob": "^3.2.4",
    "fs-extra": "^9.0.1",
    "source-map-support": "^0.5.19",
    "type-fest": "^0.16.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^9.1.2",
    "@commitlint/config-conventional": "^9.1.2",
    "@material-ui/core": "^4.11.0",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/lab": "^4.0.0-alpha.56",
    "@reach/router": "^1.3.4",
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/git": "^9.0.0",
    "@semantic-release/release-notes-generator": "^9.0.1",
    "@types/classnames": "^2.2.10",
    "@types/lodash-es": "^4.17.3",
    "@types/reach__router": "^1.3.5",
    "@types/react": "^16.9.46",
    "@types/react-dom": "^16.9.8",
    "@types/react-redux": "^7.1.9",
    "@types/redux-actions": "^2.6.1",
    "@typescript-eslint/eslint-plugin": "^3.9.1",
    "@typescript-eslint/parser": "^3.9.1",
    "bootstrap": "^4.5.2",
    "classnames": "^2.2.6",
    "compare-versions": "^3.6.0",
