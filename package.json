{
  "name": "papyrus-compiler-electron",
  "productName": "papyrus-compiler-app",
  "version": "1.2.0",
  "description": "App to compile papyrus scripts",
  "main": "main/index-dev.js",
  "scripts": {
    "start": "electron-forge start",
    "start:angular": "cd app/papyrus-compiler-app && yarn start",
    "package": "cross-env NODE_ENV=production cross-env DEBUG=electron-packager node ./package.js"
  },
  "keywords": [],
  "author": "lyrod",
  "license": "MIT",
  "config": {
    "forge": {
      "make_targets": {
        "win32": [
          "zip"
        ]
      },
      "electronPackagerConfig": {
        "packageManager": "yarn"
      },
      "electronWinstallerConfig": {
        "name": "papyrus_compiler_electron"
      },
      "electronInstallerDebian": {},
      "electronInstallerRedhat": {},
      "github_repository": {
        "owner": "",
        "name": ""
      },
      "windowsStoreConfig": {
        "packageName": "",
        "name": "papyruscompilerelectron"
      }
    }
  },
  "dependencies": {
    "electron": "^4.0.5",
    "electron-compile": "^6.4.4"
  },
  "devDependencies": {
    "@babel/core": "^7.3.3",
    "@babel/plugin-transform-destructuring": "^7.3.2",
    "@babel/preset-env": "^7.3.1",
    "@types/fs-extra": "^5.0.5",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "colors": "^1.3.3",
    "cross-env": "^5.2.0",
    "electron-forge": "^5.2.4",
    "electron-prebuilt-compile": "4.0.0",
    "escape-string-regexp": "^1.0.5",
    "eslint": "^3",
    "eslint-config-airbnb": "^15",
    "eslint-plugin-import": "^2",
    "eslint-plugin-jsx-a11y": "^5",
    "eslint-plugin-react": "^7",
    "fs-extra": "^7.0.1",
    "rimraf": "^2.6.3"
  }
}
