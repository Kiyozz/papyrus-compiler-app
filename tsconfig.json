{
  "compilerOptions": {
    "jsx": "react",
    "target": "es2015",
    "module": "esnext",
    "moduleResolution": "node",
    "strict": true,
    "noEmit": true,
    "allowSyntheticDefaultImports": true,
    "skipLibCheck": true,
    "noImplicitReturns": false,
    "experimentalDecorators": false,
    "baseUrl": "./",
    "isolatedModules": true
  }
}
